%questao 4
%somatorio n
n=input('digite a constante n:');
k=input('digite a constante k:');

soma=0;
for s=-n:n
    if((s-2)~=0)
        soma=soma+(2*s+1)/(s-2);
    end
end
%somatorio k

soma1=0;
for s=-k:k
    if(s~=0&&k~=0)
        soma1=soma1+(s+1)/(k*s);
    end
end
somatorio=soma+soma1;
fprintf('%f',somatorio);